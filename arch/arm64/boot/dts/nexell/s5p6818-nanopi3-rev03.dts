/dts-v1/;
#include "s5p6818-nanopi3-common.dtsi"

/ {
	model = "FriendlyElec Smart6818";
	compatible = "friendlyelec,smart6818", "nexell,s5p6818";
};

&mach {
	hwrev = <3>;
	model = "Smart6818";
};

&dw_mmc_1 {
	status = "okay";
};

&simple_card {
	simple-audio-card,dai-link@0 {
		hp-det-gpio = <&gpio_b 27 0>;
	};
};

&spi_0 {
	/delete-node/ fbtft@0;
};
